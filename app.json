{
  "expo": {
    "name": "BroomBoom",
    "slug": "BroomBoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoSmall.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Icon/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5C001"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoSmall.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hatad.BroomBroom"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
